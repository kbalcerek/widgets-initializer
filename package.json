{
  "name": "widgets-initializer",
  "description": "Initialize widgets in DOM recursively",
  "version": "1.0.0",
  "browser": "dist/lib.js",
  "main": "dist/lib.node.js",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "anywhere": "^1.6.0",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "init-project": "node .\\node_modules\\copyfiles\\copyfiles .\\anywhere .\\node_modules\\anywhere\\bin",
    "example:1": "node .\\node_modules\\anywhere\\bin\\anywhere -u /examples/example1/index.html",
    "build": "webpack --config webpack.config.js",
